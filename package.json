{
  "name": "stakingpooljs",
  "version": "1.0.0",
  "description": "Typescript version",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon",
    "build": "rimraf dist; tsc --pretty && tscp",
    "start:prod": "NODE_ENV='production' node dist/index.js",
    "pm2": "yarn build && pm2 start pm2.config.js",
    "heroku-postbuild": "echo Skip build on Heroku",
    "format": "./node_modules/.bin/prettier --write '{**/*,*}.{js,ts,jsx,tsx}'",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "keywords": [],
  "author": "Nhan Cao <me@nhancv.com> (https://nhancv.github.io)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "typescript-cp": "^0.1.7"
  },
  "heroku-run-build-script": true
}
